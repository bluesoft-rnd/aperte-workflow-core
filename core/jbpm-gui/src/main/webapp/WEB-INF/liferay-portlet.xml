<?xml version="1.0" encoding="UTF-8"?>

<liferay-portlet-app>
    <portlet>
        <!-- Matches definition in portlet.xml. -->
        <!-- Note: Must not be the same as servlet name. -->
        <portlet-name>ProcessTool Tasks</portlet-name>
        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>
    <portlet>
        <!-- Matches definition in portlet.xml. -->
        <!-- Note: Must not be the same as servlet name. -->
        <portlet-name>ProcessTool Queues</portlet-name>
        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>

    <portlet>
        <!-- Matches definition in portlet.xml. -->
        <!-- Note: Must not be the same as servlet name. -->
        <portlet-name>ProcessTool Activities</portlet-name>
        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>

    <portlet>
        <!-- Matches definition in portlet.xml. -->
        <!-- Note: Must not be the same as servlet name. -->
        <portlet-name>ProcessTool Dictionaries</portlet-name>
        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
    </portlet>
    <portlet>
        <!-- Matches definition in portlet.xml. -->
        <!-- Note: Must not be the same as servlet name. -->
        <portlet-name>Aperte Workflow Plugin Manager</portlet-name>
        <instanceable>false</instanceable>
        <ajaxable>false</ajaxable>
        <control-panel-entry-category>portal</control-panel-entry-category>
        <control-panel-entry-weight>0.5</control-panel-entry-weight>
    </portlet>

